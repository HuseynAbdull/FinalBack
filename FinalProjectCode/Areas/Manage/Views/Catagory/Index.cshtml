@model IEnumerable<ProductType>
@{
    ViewData["Title"] = "Index";
    int count = 0;
}
<h1 class="h3 mb-4 text-gray-800">Catagorys Page</h1>
<div class="row">
    <div class="col-lg-12">
       <table class="table table-bordered table-striped">
           <thead>
               <tr>
                   <th>No</th>
                    <th>Name</th>
                    <th>Created At</th>
                    <th>Created By</th>
               </tr>
           </thead>
           <tbody>
              @foreach (ProductType productType in Model)
                {
                    count ++;
                    <tr>
                        <td>@count<td>
                        <td>@productType.Name</td>
                        <td>@productType.CreatedAt?.ToString("dd-MMM-yyyy")</td>
                        <td>@productType.CreatedBy</td>
                    </tr>
                }
           </tbody>
       </table>
    </div>

</div>